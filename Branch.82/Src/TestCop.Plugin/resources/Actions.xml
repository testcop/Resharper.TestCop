<?xml version="1.0" encoding="utf-8"?>
<actions xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns="urn:shemas-jetbrains-com:actions-xml">
  <action id="TestCop.JumpToTest" text="Jump to and from test file" >
    <shortcut>Control+G Control+T</shortcut>
    <vs-shortcut>Control+G Control+T</vs-shortcut>
  </action>
  <action id="Testcop.UnitTestRunForContext" text="Testcop Run Unit Tests" >
    <shortcut>Control+U Control+T</shortcut>
    <vs-shortcut>Control+G Control+X</vs-shortcut>
  </action>
  
  <insert group-id="ReSharper" position="last">        
      <action-ref id="TestCop.JumpToTest" />    
  </insert>
 <!--
 <insert group-id="ReSharper.UnitTests" position="last">
    <action-ref id="Testcop.UnitTestRunForContext" />
  </insert>
-->
</actions>